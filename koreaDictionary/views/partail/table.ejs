
<div class="col col-xxl-8 col-lg-8 main">
    <div class="box-lis">
        <table>
            <% let mongoDBs = [] %>
            <% if(mongoDBs = mongoDB){%>
                <% mongoDBs.forEach((d)=>{%>
                    <tr>
                        <td style="width: 35%;"> <%= d["word"]%> </td>
                        <td style="width: 35%;"> <%= d["ipa"]%></td>
                        <td style="width: 30%;"><a href="/?id=<%= d["_id"] %>"><button type="submit" class="submit-btn" >Edit</button></a></td>
                    </tr>
                   <%})%>
            <%}%>
        </table>
    </div>
</div>